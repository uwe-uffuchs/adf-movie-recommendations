{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-move-recommendation"
		},
		"sastagingfiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sastagingfiles'"
		},
		"Azure Databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1130-022637-n4seetmz"
		},
		"kvmovierecommendation_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvmovierecommendation.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-move-recommendation_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sastagingfiles_connectionString": {
						"value": "[parameters('sastagingfiles_connectionString')]"
					},
					"kvmovierecommendation_properties_typeProperties_baseUrl": {
						"value": "[parameters('kvmovierecommendation_properties_typeProperties_baseUrl')]"
					},
					"Azure Databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Azure Databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}